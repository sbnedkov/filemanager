{
    "rules": {
        "react/prop-types": "off",
        "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "^_"}]
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "amd": true,
        "es6": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "document": true
    }
}
